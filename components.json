    "@tweakcn": {
      "url": "https://tweakcn.com/r/{name}.json"
    },
    "@aceternity": {
      "url": "https://ui.aceternity.com/r/{name}.json"
    },
    "@originui": {
      "url": "https://originui.com/r/{name}.json"
    }
  }
