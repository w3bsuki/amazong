{
  "name": "amazong-marketplace",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.15.4",
  "scripts": {
    "docs": "cd docs-site && pnpm dev",
    "docs:install": "cd docs-site && pnpm install",
    "validate:skills": "node scripts/validate-agent-skills.mjs",
    "styles:scan:palette": "node scripts/scan-tailwind-palette.mjs",
    "styles:scan:arbitrary": "node scripts/scan-tailwind-arbitrary.mjs",
    "styles:scan": "pnpm -s styles:scan:palette && pnpm -s styles:scan:arbitrary",
    "styles:gate": "cross-env FAIL_ON_FINDINGS=1 pnpm -s styles:scan",
    "build": "next build",
    "dev": "next dev -H 0.0.0.0",
    "dev:webpack": "next dev -H 0.0.0.0 --webpack",
    "lint": "eslint .",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "ts:gate": "node scripts/ts-safety-gate.mjs",
    "ts:gate:baseline": "node scripts/ts-safety-gate.mjs --update-baseline",
    "start": "next start",
    "analyze": "ANALYZE=true next build",
    "test": "pnpm test:unit && pnpm test:e2e:smoke",
    "test:e2e": "node scripts/run-playwright.mjs test --project=chromium --grep-invert @accessibility",
    "test:e2e:smoke": "node scripts/run-playwright.mjs test e2e/smoke.spec.ts --project=chromium",
    "test:e2e:auth": "node scripts/run-playwright.mjs test e2e/auth.spec.ts --project=chromium",
    "test:e2e:all": "node scripts/run-playwright.mjs test --grep-invert @accessibility",
    "test:e2e:headed": "node scripts/run-playwright.mjs test --project=chromium --headed --grep-invert @accessibility",
    "test:a11y": "node scripts/run-playwright.mjs test --project=accessibility",
    "test:lighthouse": "pnpm build && lhci autorun",
    "test:lighthouse:local": "lhci collect && lhci assert",
    "test:prod": "pnpm build && cross-env TEST_PROD=true pnpm test:e2e && pnpm test:a11y && pnpm test:lighthouse",
    "test:ci": "pnpm test:unit && pnpm build && playwright test",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "test:all": "pnpm -s lint && pnpm -s ts:gate && pnpm -s typecheck && pnpm -s test:unit && pnpm -s test:e2e && pnpm -s test:a11y",
    "test:full": "pnpm -s lint && pnpm -s typecheck && pnpm -s test:unit && pnpm -s test:prod",
    "ux:screenshot": "node scripts/ux-audit-screenshot.mjs",
    "clean:artifacts": "node scripts/clean-artifacts.mjs",
    "clean:artifacts:dry": "node scripts/clean-artifacts.mjs --dry-run",
    "clean:next": "node -e \"require('node:fs').rmSync('.next',{recursive:true,force:true})\"",
    "clean": "pnpm -s clean:artifacts",
    "knip": "knip",
    "knip:fix": "knip --fix",
    "knip:watch": "knip --watch",
    "dupes": "jscpd ./app ./components ./lib --min-lines 10 --reporters console --gitignore",
    "cap:build": "next build && npx cap sync",
    "cap:android": "npx cap open android",
    "cap:ios": "npx cap open ios",
    "cap:sync": "npx cap sync"
  },
  "dependencies": {
    "@ai-sdk/gateway": "3.0.19",
    "@ai-sdk/google": "3.0.10",
    "@ai-sdk/openai": "3.0.14",
    "@capacitor/android": "8.0.1",
    "@capacitor/core": "8.0.1",
    "@capacitor/ios": "8.0.1",
    "@hookform/resolvers": "5.2.2",
    "@phosphor-icons/react": "2.1.10",
    "@radix-ui/react-accordion": "1.2.12",
    "@radix-ui/react-alert-dialog": "1.1.15",
    "@radix-ui/react-aspect-ratio": "1.1.8",
    "@radix-ui/react-avatar": "1.1.11",
    "@radix-ui/react-checkbox": "1.3.3",
    "@radix-ui/react-dialog": "1.1.15",
    "@radix-ui/react-dropdown-menu": "2.1.16",
    "@radix-ui/react-hover-card": "1.1.15",
    "@radix-ui/react-label": "2.1.8",
    "@radix-ui/react-popover": "1.1.15",
    "@radix-ui/react-progress": "1.1.8",
    "@radix-ui/react-radio-group": "1.3.8",
    "@radix-ui/react-scroll-area": "1.2.10",
    "@radix-ui/react-select": "2.2.6",
    "@radix-ui/react-separator": "1.1.8",
    "@radix-ui/react-slider": "1.3.6",
    "@radix-ui/react-slot": "1.2.4",
    "@radix-ui/react-switch": "1.2.6",
    "@radix-ui/react-tabs": "1.1.13",
    "@radix-ui/react-toast": "1.2.15",
    "@radix-ui/react-toggle": "1.1.10",
    "@radix-ui/react-toggle-group": "1.1.11",
    "@radix-ui/react-tooltip": "1.2.8",
    "@supabase/ssr": "0.8.0",
    "@tabler/icons-react": "3.36.1",
    "ai": "6.0.45",
    "boring-avatars": "2.0.4",
    "class-variance-authority": "0.7.1",
    "client-only": "0.0.1",
    "clsx": "2.1.1",
    "cmdk": "1.1.1",
    "date-fns": "4.1.0",
    "framer-motion": "12.27.5",
    "lucide-react": "0.562.0",
    "next": "16.1.4",
    "next-intl": "4.7.0",
    "next-themes": "0.4.6",
    "photoswipe": "5.4.4",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-dropzone": "14.3.8",
    "react-hook-form": "7.71.1",
    "react-markdown": "10.1.0",
    "recharts": "3.6.0",
    "remark-gfm": "4.0.1",
    "server-only": "0.0.1",
    "sharp": "0.34.5",
    "sonner": "2.0.7",
    "stripe": "20.2.0",
    "tailwind-merge": "3.4.0",
    "vaul": "1.1.2",
    "zod": "4.3.5"
  },
  "devDependencies": {
    "@axe-core/playwright": "4.11.0",
    "@capacitor/cli": "8.0.1",
    "@lhci/cli": "0.15.1",
    "@next/bundle-analyzer": "16.1.4",
    "@playwright/test": "1.57.0",
    "@supabase/supabase-js": "2.91.0",
    "@tailwindcss/postcss": "4.1.18",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.2",
    "@types/node": "25.0.9",
    "@types/react": "19.2.9",
    "@types/react-dom": "19.2.3",
    "@typescript-eslint/parser": "8.53.1",
    "@vitejs/plugin-react": "5.1.2",
    "@vitest/coverage-v8": "4.0.17",
    "cross-env": "10.1.0",
    "dotenv": "17.2.3",
    "eslint": "9.39.2",
    "eslint-config-next": "16.1.4",
    "eslint-plugin-sonarjs": "3.0.5",
    "eslint-plugin-unicorn": "62.0.0",
    "jscpd": "4.0.7",
    "jsdom": "27.4.0",
    "knip": "5.82.1",
    "postcss": "8.5.6",
    "postcss-load-config": "6.0.1",
    "tailwindcss": "4.1.18",
    "tailwindcss-animate": "1.0.7",
    "typescript": "5.9.3",
    "vite-tsconfig-paths": "6.0.4",
    "vitest": "4.0.17"
  }
}
