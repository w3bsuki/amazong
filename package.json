  "name": "amazong-marketplace",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "validate:skills": "node scripts/validate-agent-skills.mjs",
    "build": "next build",
    "dev": "next dev . --webpack",
    "dev:turbopack": "next dev . --turbo",
    "dev:webpack": "next dev . --webpack",
    "lint": "eslint .",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "start": "next start",
    "analyze": "ANALYZE=true next build",
    "test:e2e": "node scripts/run-playwright.mjs test --project=chromium --grep-invert @accessibility",
    "test:e2e:all": "node scripts/run-playwright.mjs test --grep-invert @accessibility",
    "test:e2e:headed": "node scripts/run-playwright.mjs test --project=chromium --headed --grep-invert @accessibility",
    "test:a11y": "node scripts/run-playwright.mjs test --project=accessibility",
    "test:lighthouse": "pnpm build && lhci autorun",
    "test:lighthouse:local": "lhci collect && lhci assert",
    "test:prod": "pnpm build && cross-env TEST_PROD=true pnpm test:e2e && pnpm test:a11y && pnpm test:lighthouse",
    "test:ci": "pnpm build && playwright test && lhci autorun",
    "test:unit": "vitest",
    "test:unit:watch": "vitest --watch",
    "test:unit:coverage": "vitest --coverage",
    "test:all": "pnpm -s lint && pnpm -s typecheck && pnpm -s test:unit && pnpm -s test:e2e && pnpm -s test:a11y",
    "test:full": "pnpm -s lint && pnpm -s typecheck && pnpm -s test:unit && pnpm -s test:prod"
  },
